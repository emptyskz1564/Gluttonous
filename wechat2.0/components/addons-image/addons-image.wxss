/* components/imagepreview/preview.wxss */
.back-panel {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.75);
  position: fixed;
  z-index: 99;
}
.image-panel {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  align-content: center;
  text-align: center;
  width: 100%;
  height: 100%;
  z-index: 99;
}
.image-panel image {
  width: 100%;
  z-index: 99;
  margin: 0 auto;
}